    "mood",
    "music",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.3.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@types/node-fetch": "^2.6.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
