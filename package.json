{
  "name": "spotify-mood-analyzer",
  "version": "1.0.0",
  "description": "Daily Spotify mood analyzer that maps listening history to elemental moods",
  "main": "fetch-daily-mood.ts",
  "scripts": {
    "get-token": "tsx get-refresh-token.ts",
    "analyze": "tsx fetch-daily-mood.ts",
    "build": "tsc",
    "start": "node dist/fetch-daily-mood.js",
    "serve": "npx http-server -p 8000"
